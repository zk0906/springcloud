@import "/utils/font_icon.wxss";
.flex-wrap{
	display:-webkit-flex;
	display:flex;
	-webkit-flex-direction:row;
	flex-direction:row;
}
.flex-item{
	flex: 1;
	-webkit-flex: 1;
}
.bottomTabBar{
	width: 100%;
	height: 98rpx;
	background-color: #fff;
	border-top: 1px solid #e9e9e9;
	position: fixed;
	left: 0;
	bottom: 0;
	z-index: 100;
}
.bottomTabBar-item{
	color: #000;
	font-size: 20rpx;
	text-align: center;
}
.bottomTabBar-img{
	display: block;
	width: 36rpx;
	height: 36rpx;
	margin: 15rpx auto 10rpx;
}
.bottomTabBar-icon{
  display: block;
	width: 36rpx;
	height: 36rpx;
  margin: 15rpx auto 10rpx;
  font-size:36rpx;
  line-height:36rpx;
  text-align:center;
}
.bottomTabBar-item.active{
	color: #FF4200;
}
.franchiseeTabbar-place{
	height: 100rpx;
	width: 100%;
	position: absolute;
	bottom: -100rpx;
}