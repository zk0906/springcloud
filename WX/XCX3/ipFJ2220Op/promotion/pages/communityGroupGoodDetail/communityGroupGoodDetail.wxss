/* 购物车 */

#communityBuyDialog .minus, #communityBuyDialog .plus {
  position: relative;
  display: inline-block;
  font-size: 16px;
  outline: 0 !important;
  text-indent: -9999px;
  overflow: hidden;
  vertical-align: middle;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-weight: normal;
  background-color: #f1f1f1;
  color: #878787;
  border-radius: 2px;
  border: none;
}

#communityBuyDialog .minus.disabled, #communityBuyDialog .plus.disabled {
  background-color: #f9f9f9;
  color: #cacaca;
}

#communityBuyDialog .quantity input {
  height: 60rpx;
  min-height: 60rpx;
  line-height: 60rpx;
}

#communityBuyDialog .minus:before, #communityBuyDialog .plus:before,
#communityBuyDialog .plus:after {
  position: absolute;
  width: 8px;
  height: 2px;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
  background-color: #878787;
  bottom: 0;
  content: '';
}

#communityBuyDialog .plus:after {
  width: 2px;
  height: 8px;
}

#communityBuyDialog .minus.disabled:before,
#communityBuyDialog .plus.disabled:before,
#communityBuyDialog .plus.disabled:after {
  background-color: #cacaca;
}

#communityBuyDialog .shopping-number {
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
}

#communityBuyDialog .page-bottom-dialog {
  height: 892rpx;
  background: #fff;
  padding: 0 20rpx;
  box-sizing: border-box;
}

#communityBuyDialog .dialog-goods-header {
  display: block;
  padding: 20rpx 0 35rpx;
  border-bottom: 1px solid #f3f3f3;
}

#communityBuyDialog .communityBuyDialog-buy-num {
  font-size: 24rpx;
  margin-top: 30rpx;
}

#communityBuyDialog .communityBuyDialog-content {
  height: 615rpx;
  overflow: auto;
}

