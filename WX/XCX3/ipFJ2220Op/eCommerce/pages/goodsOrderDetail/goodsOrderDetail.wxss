page {
  background: #F3F3F3;
  height: 100%;
}
.order-contain {
  height: calc(100% - 88rpx);
  overflow-y: auto;
  position: relative;
}
.order-contain > view {
  padding: 0 30rpx;
  background: #fff;
}
.order-contain .seller-shipped {
  height: 180rpx;
  color: #fff;
  line-height: 180rpx;
  font-size: 34rpx;
  background: linear-gradient(to right, #FFAE00 , #FF7100);
  padding: 0
}
.order-contain .seller-shipped text {
  display: inline-block;
  vertical-align: middle;
}
.order-contain .seller-shipped .order-icon {
  font-size: 40rpx;
  margin-right: 15rpx;
}
.order-contain .seller-shipped > view {
  background-image: url('http://cdn.jisuapp.cn/zhichi_frontend/static/webapp/images/xcx-goods/goods-order-bg.png');
  background-repeat: no-repeat;
  background-position: left bottom;
  background-size: 100% 100%;
  padding-left: 30rpx;
}
.order-contain .buyer-information {
  margin-bottom: 15rpx;
  padding: 30rpx;
}
.buyer-name-box {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 6rpx;
}
.buyer-name-box > text {
  vertical-align: middle;
  display: inline-block;
}
.buyer-box {
  font-size: 26rpx;
  color: #666;
  line-height: 40rpx;
  width: 630rpx;
  vertical-align: middle;
  display: inline-block
}
.goods-detail-box image{
  width: 156rpx;
  height: 156rpx;
  display: inline-block;
  vertical-align: middle;
}
.order-contain .goods-detail-box {
  padding: 30rpx;
}
.goods-detail-box > view {
  width: 506rpx;
  display: inline-block;
  vertical-align: top;
  margin-left: 28rpx;
}
.goods-name {
  font-size: 26rpx;
  color: #333;
  line-height: 36rpx;
  overflow: hidden; 
  text-overflow: ellipsis;
  display: -webkit-box; 
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  word-break: break-all;
}
.goods-rules {
  font-size: 24rpx;
  color: #999;
  margin-top: 8rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.goods-price {
  font-size: 28rpx;
  color: #333;
  margin-top: 12rpx;
}
.goods-price .pull-right {
  font-size: 26rpx;
}
.goods-amount,.promotion-method,.freight,.total-order-price,.stored-value-deduction,.spike-offer {
  font-size: 24rpx;
  color: #666;
  line-height: 42rpx;
  overflow: hidden;
}
.promotion-method .promotion-method-title {
  width: 506rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: right;
}
.order-contain .stored-value-deduction {
  padding-bottom: 20rpx ;
}
.actual-payment {
  line-height: 84rpx;
  color: #333;
  font-size: 26rpx;
  margin-bottom: 16rpx;
}
.actual-payment .colorff7 {
  font-size: 28rpx;
}
.addition-info {
  line-height: 84rpx;
  color: #333;
  font-size: 26rpx;
}
.order-contain .buyer-message {
  color: #333;
  font-size: 26rpx;
  overflow: hidden;
  padding: 20rpx 30rpx
}
.buyer-message .pull-right {
  width: 492rpx;
  line-height: 40rpx;
  text-align: right;
}
.actual-payment > view,.addition-info > view {
  border-top: 1px solid #f3f3f3;
}
.no-border > view {
  border-top: 0;
}
.already-refund-goods {
  margin-top: 16rpx;
} 
.order-contain .order-number {
  font-size: 24rpx;
  color: #666;
  padding: 15rpx 30rpx;
  margin-bottom: 30rpx;
  margin-top: 16rpx;
}
.order-number > view {
  line-height: 50rpx;
}
.order-number  text:first-child {
  margin-right: 30rpx;
}
.order-number .pull-right {
  width: 100rpx;
  border-radius: 17rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 22rpx;
  border: 1px solid #D2D2D2;
  text-align: center;
  color: #999;
  margin-top: 6rpx;
}
.order-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  line-height: 80rpx;
  text-align: right;
  box-sizing: border-box;
  border-top: 1px solid #EBEBEB;
}
.order-bottom .btn {
  border: 1px solid #D2D2D2;
  padding: 0 28rpx;
  line-height: 48rpx;
  margin-left: 20rpx;
  color: #999999;
  display: inline-block;
  font-size: 24rpx;
  border-radius: 30rpx;
}
.order-bottom .btn.btn-orange {
  border-color: #ff7100;
  color: #ff7100;
  background: #fff;
}
.color999 {
  color: #999;
}
.colorff7 {
  color: #ff7100;
}
.buyer-information .icon-goods-location {
  color: #666;
  display: inline-block;
  vertical-align: middle;
  margin-right: 16rpx;
  font-size: 34rpx;
}
.cash-delivery {
  display:inline-block;
  padding: 0 10rpx;
  background: #f2303e;
  color: #fff;
  font-size: 22rpx;
  height: 28rpx;
  line-height: 26rpx;
  margin-right: 10rpx;
  border-radius: 4rpx;
}
.shipping-type {
  background: #ff7100;
}
.is-seckill {
  display:inline-block;
  padding: 0 10rpx;
  background: #f2303e;
  color: #fff;
  font-size: 22rpx;
  height: 26rpx;
  line-height: 26rpx;
  margin-right: 10rpx;
  border-radius: 4rpx;
}
.app-box {
  line-height: 84rpx;
}
.app-box .app-logo{
  width: 44rpx;
  height: 44rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.app-box .app-name {
  font-size: 26rpx;
  color: #333; 
  vertical-align: middle;
}
.app-box .contact-service {
  font-size: 24rpx;
  color: #999;
  display: inline-block;
  width: 130rpx;
  position: relative;
  height: 82rpx;
}
.contact-service .icon-goods-customer-service {
 margin-left: 8rpx;
 font-size: 26rpx;
 color: #D2D2D2;
}
.app-box .contact-service button{
  position: absolute;
  width: 100%;
  height: 100%;
  opacity: 0;
  left: 0;
  top: 0;
}
.app-box > view {
  border-bottom: 1px solid #F3F3F3;
}
.icon-rightarrow {
  font-size: 20rpx;
}
.order-contain .orderDetail-goto-homepage {
  position: absolute;
  display: inline-block;
  color: #FF7100;
  right: 0;
  top: 132rpx;
  font-size: 24rpx;
  background: rgba(255, 255, 255, 0.8);
  width: 150rpx;
  text-align: center;
  line-height: 50rpx;
  padding: 0;
  border-radius: 4rpx 0 0 0;
}
/* 核销码 */
.verification-code {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  text-align: center;
}
.verification-code-img {
  width: 100%;
  height: 100%;
}
.verification-code-text {
  color: #fff;
  margin-top: 20rpx;
  font-size: 30rpx;
}
.apply-refund {
  float: left;
  width: 150rpx;
  font-size: 24rpx;
  text-align: left;
  color: #666;
}

.event-dialog-mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  text-align: center;
}
.event-dialog {
  width: 560rpx;
  height: 230rpx;
  background: #fff;
  margin-top: 350rpx;
  display: inline-block;
  border-radius: 10rpx;
}
.event-dialog-title {
  height: 150rpx;
  font-size: 28rpx;
  line-height: 150rpx;
}
.event-dialog-bottom {
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid #EBEBEB;
  font-size: 30rpx;
}
.event-dialog-bottom > text {
  width: 50%;
  display: inline-block;
  box-sizing: border-box;
}
.event-dialog-bottom .event-cancel {
  border-right: 1rpx solid #EBEBEB;
  color: #666666;
}
.event-dialog-bottom .event-sure {
  color: #FF7100;
}
.verification-image-box {
  width: 460rpx;
  height: 460rpx;
  margin-top: 30rpx;
  display: inline-block;
  position: relative;
}
.verification-code-finished {
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 2555, 0.6);
  position: absolute;
  top: 0;
  left: 0;
  overflow: hidden;
}
.verification-code-finished > view {
  position: absolute;
  width: 200rpx;
  height: 200rpx;
  top: -100rpx;
  left: -100rpx;
  color: #fff;
  background: #F2303E;
  transform: rotate(-45deg);
  line-height: 325rpx;
  font-size: 26rpx;
}
.order-contain .order-prompt {
  padding: 24rpx 30rpx;
  line-height: 37rpx;
  color: #333;
  font-size: 26rpx;
  margin-bottom: 15rpx;
}
.order-prompt text {
  color: #ff7100;
}
.order-contain .refund-address-box {
  padding: 20rpx 30rpx;
  margin-bottom: 16rpx;
  overflow: hidden;
}
.refund-address-title {
  color: #333;
  font-size: 26rpx;
}
.refund-address {
  color: #666;
  text-align: right;
  width: 520rpx;
  font-size: 24rpx;
  line-height: 37rpx;
  margin-top:10rpx;
  word-break:break-all;
}
.buyer-address {
  overflow: hidden; 
  text-overflow: ellipsis;
  display: -webkit-box; 
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  line-height: 37rpx;
}
.buyer-name {
  overflow: hidden;
  max-width: 300rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-right: 20rpx;
}
.verification-code-tips {
  font-size: 32rpx;
  color: #fff;
  margin-top: 290rpx;
}
.goods-logistics-title{
  font-size: 28rpx;
  color: #ff7100;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
}
.goods-logistics-time {
  color: #999;
  font-size: 26rpx;
  margin-top: 15rpx;
}
.order-contain .goods-logistics {
  padding: 40rpx 0;
  border-bottom: 1px solid #F3F3F3;
}
.goods-logistics .icon-goods-logistics {
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.goods-logistics-info {
  display: inline-block;
  vertical-align: middle;
  width:calc(100% - 80rpx);
}
.refunding-btn {
  float: right;
  width: 128rpx;
  line-height: 48rpx;
  border: 1px solid #D2D2D2;
  text-align: center;
  color: #666;
  border-radius: 30rpx;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}
.refunding-btn-box {
  overflow: hidden;
}
.self-delivery-info .buyer-box{
  width: 550rpx;
}
.order-contain .self-delivery-info {
  padding-right: 0;
}
.delivery-navigation {
  display:inline-block;
  vertical-align:middle;
  width:110rpx;
  text-align:center;
  border-left:1px solid #F3F3F3;
  font-size:26rpx;
  color:#ff7100;
}
.colord2d2 {
  color: #D2D2D2;
}
.seckill-time{
  width: 100%;
  height: 60rpx;
  background: #FFF6EE !important;
  display:flex;
  align-items:center;
  justify-content:center;
  padding: 0 !important;
}
.countdown{
  text-align: center;
  font-size: 20rpx;
  color: #FF9910;
}
.countdown > label{
  margin-right: 20rpx;
}
.countdown > text{
  display:inline-block;
  min-width:14px;
  color:#ffffff;
  text-align:center;
  margin:2px 8rpx;
  padding:6rpx 10rpx;
  border-radius:4px;
  vertical-align:middle;
  background: #FF9910;
}