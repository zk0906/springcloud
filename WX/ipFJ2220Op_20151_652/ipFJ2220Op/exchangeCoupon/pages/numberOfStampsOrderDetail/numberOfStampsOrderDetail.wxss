page {
  background: #F3F3F3;
  height: 100%;
}
.order-contain {
  display: none;
  height: calc(100% - 88rpx);
  overflow-y: auto;
  position: relative;
}
.order-contain.show {
  display: block;
}
.order-contain > view {
  padding: 0 30rpx;
  background: #fff;
}
.order-contain .seller-shipped {
  height: 180rpx;
  color: #fff;
  padding: 0;
  line-height: 180rpx;
  font-size: 34rpx;
  background: linear-gradient(to right, #FFAE00 , #FF7100);
}
.order-contain .seller-shipped text {
  display: inline-block;
  vertical-align: middle;
}
.order-contain .seller-shipped .order-icon {
  font-size: 40rpx;
  margin-right: 15rpx;
}
.order-contain .seller-shipped > view {
  background-image: url('http://cdn.jisuapp.cn/zhichi_frontend/static/webapp/images/xcx-goods/goods-order-bg.png');
  background-repeat: no-repeat;
  background-position: left bottom;
  background-size: 100% 100%;
  padding-left: 30rpx;
}
.order-contain .buyer-information {
  margin-bottom: 15rpx;
  padding: 30rpx;
}
.buyer-name-box {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 6rpx;
}
.buyer-name-box > text {
  vertical-align: middle;
  display: inline-block;
}
.buyer-box {
  font-size: 26rpx;
  color: #666;
  line-height: 40rpx;
  width: 630rpx;
  vertical-align: middle;
  display: inline-block
}
.goods-detail-box image{
  width: 156rpx;
  height: 156rpx;
  display: inline-block;
  vertical-align: middle;
}
.order-contain .goods-detail-box {
  padding: 30rpx;
}
.goods-detail-box > view {
  width: 506rpx;
  display: inline-block;
  vertical-align: top;
  margin-left: 28rpx;
}
.goods-name {
  font-size: 26rpx;
  color: #333;
  line-height: 36rpx;
  overflow: hidden; 
  text-overflow: ellipsis;
  display: -webkit-box; 
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  word-break: break-all;
}
.goods-rules {
  font-size: 22rpx;
  color: #999;
  margin-top: 8rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.goods-price {
  font-size: 26rpx;
  color: #333;
  margin-top: 12rpx;
}
.goods-amount,.promotion-method,.freight,.total-order-price,.stored-value-deduction,.spike-offer {
  font-size: 22rpx;
  color: #666;
  line-height: 42rpx;
  overflow: hidden;
}
.promotion-method .promotion-method-title {
  width: 506rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: right;
}
.order-contain .stored-value-deduction {
  padding-bottom: 20rpx ;
}
.actual-payment {
  line-height: 84rpx;
  color: #333;
  font-size: 26rpx;
  margin-bottom: 16rpx;
}
.actual-payment .colorff7 {
  font-size: 28rpx;
}
.addition-info {
  line-height: 84rpx;
  color: #333;
  font-size: 26rpx;
}
.order-contain .buyer-message {
  color: #333;
  font-size: 26rpx;
  overflow: hidden;
  padding: 20rpx 30rpx
}
.buyer-message .pull-right {
  width: 492rpx;
  line-height: 40rpx;
  text-align: right;
}
.actual-payment > view,.addition-info > view {
  border-top: 1px solid #f3f3f3;
}
.no-border > view {
  border-top: 0;
}
.already-refund-goods {
  margin-top: 16rpx;
} 
.order-contain .order-number {
  font-size: 26rpx;
  color: #666;
  height: 26rpx;
  padding: 30rpx;
  margin-bottom: 30rpx;
  margin-top: 16rpx;
}
.order-number > view {
  line-height: 33rpx;
}
.order-number  text:first-child {
  margin-right: 30rpx;
}
.order-number .pull-right {
  width: 100rpx;
  border-radius: 17rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 22rpx;
  border: 1px solid #D2D2D2;
  text-align: center;
  color: #999;
  margin-top: -2rpx;
}
.order-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  line-height: 80rpx;
  text-align: right;
  box-sizing: border-box;
  border-top: 1px solid #EBEBEB;
}
.order-bottom .btn {
  border: 1px solid #D2D2D2;
  padding: 0 28rpx;
  line-height: 48rpx;
  margin-left: 20rpx;
  color: #999999;
  display: inline-block;
  font-size: 24rpx;
  border-radius: 30rpx;
}
.order-bottom .btn.btn-orange {
  border-color: #ff7100;
  color: #ff7100;
  background: #fff;
}
.color999 {
  color: #999;
}
.color333 {
  color: #333;
}
.colorff7 {
  color: #FF7100;
}
.buyer-information .icon-goods-location {
  color: #333;
  display: inline-block;
  vertical-align: middle;
  margin-right: 16rpx;
  font-size: 34rpx;
}
.cash-delivery {
  display:inline-block;
  padding: 0 10rpx;
  background: #f2303e;
  color: #fff;
  font-size: 22rpx;
  height: 28rpx;
  line-height: 28rpx;
  margin-right: 10rpx;
  border-radius: 4rpx;
}
.shipping-type {
  background: #ff7100;
}
.is-seckill {
  display:inline-block;
  padding: 0 10rpx;
  background: #f2303e;
  color: #fff;
  font-size: 22rpx;
  height: 26rpx;
  line-height: 26rpx;
  margin-right: 10rpx;
  border-radius: 4rpx;
}
.app-box {
  line-height: 84rpx;
}
.app-box .app-logo{
  width: 44rpx;
  height: 44rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.app-box .app-name {
  font-size: 26rpx;
  color: #333; 
  vertical-align: middle;
}
.app-box .contact-service {
  font-size: 24rpx;
  color: #999;
  display: inline-block;
  position: relative;
  height: 82rpx;
}
.contact-service .icon-goods-customer-service {
 margin-left: 8rpx;
}
.app-box .contact-service button{
  position: absolute;
  width: 100%;
  height: 100%;
  opacity: 0;
  left: 0;
  top: 0;
}
.app-box > view {
  border-bottom: 1px solid #F3F3F3;
}
.icon-rightarrow {
  font-size: 20rpx;
}
.order-contain .orderDetail-goto-homepage {
  position: absolute;
  display: inline-block;
  color: #FF7100;
  right: 0;
  top: 132rpx;
  font-size: 24rpx;
  background: rgba(255, 255, 255, 0.8);
  width: 150rpx;
  text-align: center;
  line-height: 50rpx;
  padding: 0;
  border-radius: 4rpx 0 0 0;
}
/* 核销码 */
.verification-code {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  text-align: center;
}
.verification-code-img {
  width: 100%;
  height: 100%;
}
.verification-code-text {
  color: #fff;
  margin-top: 20rpx;
  font-size: 30rpx;
}
.apply-refund {
  float: left;
  width: 150rpx;
  font-size: 24rpx;
  text-align: left;
  color: #666;
}

.event-dialog-mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  text-align: center;
}
.event-dialog {
  width: 560rpx;
  height: 230rpx;
  background: #fff;
  margin-top: 350rpx;
  display: inline-block;
  border-radius: 10rpx;
}
.event-dialog-title {
  height: 150rpx;
  font-size: 28rpx;
  line-height: 150rpx;
}
.event-dialog-bottom {
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid #EBEBEB;
  font-size: 30rpx;
}
.event-dialog-bottom > text {
  width: 50%;
  display: inline-block;
  box-sizing: border-box;
}
.event-dialog-bottom .event-cancel {
  border-right: 1rpx solid #EBEBEB;
  color: #666666;
}
.event-dialog-bottom .event-sure {
  color: #FF7100;
}
.verification-image-box {
  width: 460rpx;
  height: 460rpx;
  margin-top: 310rpx;
  display: inline-block;
  position: relative;
}
.verification-code-finished {
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 2555, 0.6);
  position: absolute;
  top: 0;
  left: 0;
  overflow: hidden;
}
.verification-code-finished > view {
  position: absolute;
  width: 200rpx;
  height: 200rpx;
  top: -100rpx;
  left: -100rpx;
  color: #fff;
  background: #F2303E;
  transform: rotate(-45deg);
  line-height: 325rpx;
  font-size: 26rpx;
}
.order-contain .order-prompt {
  padding: 24rpx 30rpx;
  line-height: 37rpx;
  color: #333;
  font-size: 26rpx;
  margin-bottom: 15rpx;
}
.order-prompt text {
  color: #ff7100;
}
.order-contain .refund-apply-time {
  padding-bottom: 10rpx;
}
.order-contain .refund-address-box {
  padding: 20rpx 30rpx;
  margin-bottom: 16rpx;
  overflow: hidden;
}
.refund-address-title {
  color: #333;
  font-size: 26rpx;
}
.refund-address {
  color: #666;
  text-align: right;
  width: 520rpx;
  font-size: 24rpx;
  line-height: 37rpx;
  margin-top:10rpx;
}
.buyer-address {
  overflow: hidden; 
  text-overflow: ellipsis;
  display: -webkit-box; 
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  line-height: 37rpx;
}
.buyer-name {
  overflow: hidden;
  width: 300rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.order-contain .order-used {
  font-size: 24rpx;
  color: #666;
  padding: 0;
  margin-bottom: 16rpx;
  margin-top: 16rpx;
}
.order-used .order-used-title {
  color:rgba(153,153,153,1);
  line-height:37px;
  padding: 15rpx 30rpx;
  border-bottom: 1px solid #EBEBEB;
}
.order-used .order-used-title view {
  position:absolute;
  right:20px;  
}
.order-used .order-used-line {
  margin: 0 36rpx;
  display: flex;
  font-size:26rpx;
  height:26rpx;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 15rpx;
}
.order-used-line.order-used-bottom {
  border-bottom: 1px solid #f3f3f3;
}
.coupon-verify-wrap {
  display: none;
  margin: 45rpx 30rpx 0;
}
.coupon-verify-wrap.show {
  display: block;
}
.coupon-verify-wrap .top-section {
  position: relative;
  width: 100%;
  height: 320rpx;
  background: #fff;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  color: #8b8b8b;
  box-shadow: 0px -5px 5px -5px rgba(0, 0, 0, 0.2),
              5px 0px 5px -5px rgba(0, 0 , 0, 0.2),
              -5px 0px 5px -5px rgba(0, 0 , 0, 0.2);
}
.coupon-verify-wrap .top-section .cancel {
  position: absolute;
  top: 45rpx;
  left: 45rpx;
  color: #333;
}
.coupon-verify-wrap .top-section .name {
  padding-top: 140rpx;
  text-align: center;
  color: #666;
  font-size: 35rpx;
}
.coupon-verify-wrap .top-section .title {
  padding-top: 25rpx;
  text-align: center;
  color:#333;
  font-size: 60rpx;
}
.coupon-verify-wrap .division-line {
  position: relative;
  width: 100%;
  border-top: 1px dotted #dbdbdb;
}
.coupon-verify-wrap .division-line .left-circle{
  position:absolute;
  top:-5px;
  left:-5px;
  width:10px;
  height:10px;
  border-radius:50%;
  background:#eee;
}
.coupon-verify-wrap .division-line .right-circle{
  position:absolute;
  top:-5px;
  right:-5px;
  width:10px;
  height:10px;
  border-radius:50%;
  background:#eee;
}
.coupon-verify-wrap .bottom-section {
  background: #fff;
  box-shadow: 5px 0px 5px -5px rgba(0, 0 , 0, 0.2), -5px 0px 5px -5px rgba(0, 0 , 0, 0.2);
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  z-index: 1;
  text-align: center;
}
.coupon-verify-wrap .bottom-section::after {
  content:'';
  position:absolute;
  bottom:0;
  width:100%;
  height:22px;
  border-bottom-left-radius:4px;
  border-bottom-right-radius:4px;
  box-shadow:0px 0px 5px 0px rgba(0, 0 , 0, 0.2);
  z-index:-1;
}
.coupon-verify-wrap .bottom-section .verify-block {
  height: 550rpx;
}
.coupon-verify-wrap .bottom-section .success-block {
  height: 350rpx;
}
.coupon-verify-wrap .bottom-section .qrcode {
  display: inline-block;
  width: 280rpx;
  height: 280rpx;
  padding-top: 50rpx;
}
.coupon-verify-wrap .bottom-section .code {
  margin-top:15rpx;
  color:#333;
  font-size:35rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.coupon-verify-wrap .bottom-section .tip {
  margin-top:18rpx;
  font-size:28rpx;
  color:#666;
}
.coupon-verify-wrap .bottom-section .success-tip {
  padding-top: 75rpx;
}
.coupon-verify-wrap .bottom-section .success-tip-icon {
  display: inline-block;
  vertical-align: middle;
}
.coupon-verify-wrap .bottom-section .success-tip-text {
  display: inline-block;
  height: 24px;
  line-height: 24px;
  font-size: 20px;
  margin-left: 8px;
  vertical-align: middle;
}
.coupon-verify-wrap .bottom-section .back-tip {
  margin-top: 28rpx;
  font-size: 28rpx;
  color:#666;
}
.coupon-cerification-info {
  display: none;
  /* margin: 45rpx 0rpx 0; */
}
.coupon-cerification-info.show {
  display: block;
}
.coupon-cerification-info .ceification-info-title {
  color: #999;
  font-size: 26rpx;
  height: 60rpx;
  padding: 0rpx 30rpx;
  line-height: 60rpx;
  background: #fafafa;
}
.coupon-cerification-info .ceification-info-left {
  width: 50%;
  text-align: center;
  display: inline-block;
}
.coupon-cerification-info .ceification-info-line {
  height: 86rpx;
  font-size: 26rpx;
  line-height: 86rpx;
  color: #333;
}
.coupon-cerification-info .top-section {
  position: relative;
  width: 100%;
  padding: 20rpx 30rpx 20rpx;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  color: #8b8b8b;
}
.ceification-info-back {
  width:617rpx;
  height:86rpx;
  background:rgba(255,113,0,1);
  border-radius:43rpx;
  margin-top: 40rpx;
  -webkit-border-radius:43rpx;
  -moz-border-radius:43rpx;
  -ms-border-radius:43rpx;
  -o-border-radius:43rpx;
  text-align: center;
  line-height: 86rpx;
  font-size: 30rpx;
  color: #fff;
  display: inline-block;
}
.coupon-cerification-nodata {
  font-size: 26rpx;
  text-align: center;
  color: #666;
  margin: 135rpx 0 70rpx;
}