  width: 100%;
  height: 750rpx;
}
.image-video-menu {
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  margin-left: -90rpx;
  z-index: 2;
}
.image-video-menu text {
  display: inline-block;
  width: 80rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  background: rgba(0,0,0,0.2);
  color: #eee;
  text-align: center;
  border-radius: 20rpx;
} 
.image-video-menu text:first-child {
  margin-right: 20rpx;
}
.image-video-menu text.active {
  background: linear-gradient(to right, #FFAC0C, #FF7100);
  color: #fff;
}
.image-video-menu cover-view {
  display: inline-block;
  width: 80rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  background: rgba(0,0,0,0.2);
  color: #eee;
  text-align: center;
  border-radius: 20rpx;
} 
.image-video-menu cover-view:first-child {
  margin-right: 20rpx;
}
.image-video-menu cover-view.active {
  background: #FF7100;
