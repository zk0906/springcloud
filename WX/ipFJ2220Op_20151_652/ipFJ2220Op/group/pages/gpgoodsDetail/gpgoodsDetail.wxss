  align-items: center;
  width: 104rpx;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 22rpx;
  background: rgba(255, 54, 0, 1);
  color: #fff;
  border: 1px solid #ff3600;
  border-radius: 2rpx;
}

.groupMaskPrice .type .ico-moon {
  display: flex;
  color: rgba(255,54,0,1);
  font-size: 28rpx;
  margin-right: 8rpx;
  background: #fff;
}

.groupMaskPrice .leader_price, .groupMaskPrice .member_price {
  font-size: 30rpx;
  margin: 0 14rpx;
  color: #ff3600;
}

.groupMaskPrice .price {
  font-size: 24rpx;
  color: #999;
  text-decoration: line-through;
}

.pay-goods-stock {
  display: inline-block;
  margin: 14rpx 0 10rpx;
  font-size: 24rpx;
  color: #999;
}

.pay-checked-text {
  font-size: 24rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

#payDialog .payDialog-content, #groupPayDialog .payDialog-content {
  height: 615rpx;
  overflow: auto;
  margin-bottom: 100rpx;
} 

.model-title {
  display: block;
  margin-bottom: 18rpx;
  font-size: 26rpx;
  line-height: 1;
}

.pay-goods-models .pills-list {
  display: inline-block;
  vertical-align: middle;
}

.pay-goods-models > view {
  margin: 0;
  padding: 30rpx 0 30rpx;
  border-bottom: 1px solid #f3f3f3;
}

#payDialog .payDialog-buy-num, #groupPayDialog .payDialog-buy-num {
  padding: 0 10rpx 30rpx;
  font-size: 26rpx;
  margin-top: 30rpx;
  color: #333;
}

#payDialog .dialog-bottom, #groupPayDialog .dialog-bottom {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 999;
}

.pay-buy-next, .pay-add-to-shoppingcart {
  line-height: 88rpx;
  height: 88rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
}

.pay-goods-models .pills-list {
  display: inline-block;
  vertical-align: middle;
}

.pills-list label {
  box-sizing: border-box;
  color: #353535;
  margin-bottom: 20rpx;
  margin-top: 2px;
  padding: 0 20rpx;
  height: 52rpx;
  line-height: 50rpx;
  background-color: #f2f2f2;
  border-radius: 4rpx;
  font-size: 24rpx;
  border: 1px solid #f2f2f2;
}

.pills-list label.select {
  line-height: 50rpx;
  position: relative;
  background-color: rgba(255, 54, 0, 0.06);
  border: 1px solid #ff3600;
  color: #ff3600;
}

.pills-list label.select:after {
  content: '';
  position: absolute;
  bottom: 0;
  right: 0;
  width: 12px;
  height: 12px;
}

#payDialog .minus, #payDialog .plus, #groupPayDialog .minus,
#groupPayDialog .plus {
  position: relative;
  display: inline-block;
  font-size: 16px;
  outline: 0 !important;
  text-indent: -9999px;
  overflow: hidden;
  vertical-align: middle;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-weight: normal;
  background-color: #f1f1f1;
  color: #878787;
  border-radius: 2px;
  border: none;
}

#payDialog .quantity input, 
#groupPayDialog .quantity input {
  box-sizing: border-box;
  height: 60rpx;
  line-height: 58rpx;
  width: 60rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: #333;
  background: #f5f5f5;
  margin: 0 4rpx;
}

#payDialog .minus.disabled, #payDialog .plus.disabled,
#groupPayDialog .minus.disabled, #groupPayDialog .plus.disabled {
  background-color: #f9f9f9;
  color: #cacaca;
}

#payDialog .minus:before, #payDialog .plus:before, #payDialog .plus:after,
#groupPayDialog .minus:before, #groupPayDialog .plus:before,
#groupPayDialog .plus:after {
  position: absolute;
  width: 8px;
  height: 2px;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
  background-color: #878787;
  bottom: 0;
  content: '';
}

#payDialog .minus.disabled:before, #payDialog .plus.disabled:before,
#payDialog .plus.disabled:after, #groupPayDialog .minus.disabled:before,
#groupPayDialog .plus.disabled:before, #groupPayDialog .plus.disabled:after {
  background-color: #cacaca;
}

#payDialog .plus:after, #groupPayDialog .plus:after {
  width: 2px;
  height: 8px;
}

.form-contain button {
  width: 100%;
  padding: 0;
  margin: 0;
  line-height: 30rpx;
  color: #666;
  background-color: #fff;
}

.bottom-border-right {
  border-right: 1px solid #f2f2f2;
}

.goods-opt-item {
  flex: 1;
  position: relative;
  margin: -2px 0;
  font-size: 0;
  color: #666;
}

.goods-opt-item view {
  font-size: 12px;
}

contact-button {
  position: absolute;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}

.collage-bottom .ico-moon {
  width: 20px;
  font-size: 20px;
}

.icon-collect-solid {
  color: #ff7100;
}

.noGroupSureMask {
  display: flex;
}

.noGroupSureMask>view {
  flex: 1;
}

.noGroupSureMask .pay-add-to-shoppingcart {
  background: #ffac0c;
}

.noGroupSureMask .pay-buy-next {
  background: #ff7100;
}

.groupSureMask .pay-add-to-shoppingcart {
  background: #ff3600;
}

.comment-user-photo {
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
  margin-right: 7px;
  vertical-align: middle;
}

.comment-content {
  margin: 20rpx 0 20rpx;
  font-size: 26rpx;
